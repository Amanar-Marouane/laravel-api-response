{
    "name": "unk/laravel-api-response",
    "description": "Laravel API Response helpers for standardized API responses",
    "type": "library",
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/http": "^8.0|^9.0|^10.0",
        "illuminate/pagination": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "suggest": {
        "yajra/laravel-datatables-oracle": "Required for using the HttpResponseWithDataTables trait"
    },
    "autoload": {
        "psr-4": {
            "Unk\\LaravelApiResponse\\": "src/"
        }
    },
    "license": "MIT",
    "minimum-stability": "stable"
}